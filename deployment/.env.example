# PostgreSQL
POSTGRES_PASSWORD=your_super_secret_postgres_password_change_this

# JWT Secret (use: openssl rand -base64 32)
JWT_SECRET=your_super_secret_jwt_secret_change_this_use_openssl_rand

# Supabase Keys (generate using scripts/generate-keys.sh)
ANON_KEY=your_anon_key_here
SERVICE_ROLE_KEY=your_service_role_key_here

# URLs - Replace with your domain
SITE_URL=https://yourdomain.com
API_EXTERNAL_URL=https://api.yourdomain.com
ADDITIONAL_REDIRECT_URLS=https://yourdomain.com/**,http://localhost:8080/**

# MinIO
MINIO_ROOT_USER=admin
MINIO_ROOT_PASSWORD=your_secure_minio_password_change_this
MINIO_BUCKET_NAME=exam-files

# Frontend Environment Variables (copy to ../frontend/.env.production)
VITE_SUPABASE_URL=https://api.yourdomain.com
VITE_SUPABASE_PUBLISHABLE_KEY=your_anon_key_here
VITE_SUPABASE_PROJECT_ID=self-hosted
VITE_MINIO_PRESIGNER_URL=https://yourdomain.com/api/presigner
